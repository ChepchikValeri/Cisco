#include"stdafx.h"
#include <iostream>
#include <set>
#include "Cisco.h"

using namespace std;

int main()
{
	set<int> s1;
	set<int> s2;
	
	unsigned start_value;
	cin >> start_value;
	unsigned stop_value;
	cin >> stop_value;

	for (unsigned int i = start_value; i <= stop_value; i++)
	{
		if (i % 2)
			s2.insert(i);
		else
			s1.insert(i);

	}
	//your code

	set<int>::iterator it1 = s1.begin();
	set<int>::iterator it2 = s2.begin();	
	set<int>::reverse_iterator it11 = s1.rbegin();
	set<int>::reverse_iterator it22 = s2.rbegin();
	for (;it1 != s1.end()  ; ++it1)
	{
		for (it2 = s2.begin(); it2 != s2.end(); ++it2)
		{
			cout << *it1 + *it2 << " ";
		}
		cout << endl;
	}
	for (; it11 != s1.rend(); ++it11)
	{
		for (it22 = s2.rbegin(); it22 != s2.rend(); ++it22)
		{
			cout << *it11 + *it22 << " ";
		}
		cout << endl;
	}
	for (auto i:s1)
	{
		for (auto j:s2)
		{
			cout << i + j << " ";
		}
		cout << endl;
	}
	cout << endl;
	system("pause");
	return 0;
}
