#include "stdafx.h"
#include <iostream>
#include <map>
#include <string>

using namespace std;


int main()
{
	map<string, string> capitals = {
		{ "China", "Beijing" },
	{ "India", "New Delhi" },
	{ "Japan", "Tokyo" },
	{ "Philippines", "Manila" },
	{ "Russia", "Moscow" },
	{ "Egypt", "Cairo" },
	{ "Indonesia", "Jakarta" },
	{ "Democratic Republic of the Congo", "Kinshasa" },
	{ "South Korea", "Seoul" },
	{ "Bangladesh", "Dhaka" }
	};
	string s;
	getline(cin, s);
	
	map<string, string>::iterator found;
	if (capitals.find(s) != capitals.end())
		cout << "The capital of " << s << " is " << capitals.find(s)->second << endl;
	else	
		cout << "Sorry, unknown country" << endl;
	
	system("pause");
	return 0;
}
