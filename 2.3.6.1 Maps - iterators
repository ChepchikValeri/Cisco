#include"stdafx.h"
#include <iostream>
#include <set>
#include "Cisco.h"
#include <fstream>
#include <set>
#include <map>
#include <vector>
#include <functional>
#include <string>
#include <algorithm>

using namespace std;


int main()
{
	map<string, int> variables_map = { { "x", 0 },{ "y", 1 },{ "z", 2 },
	{ "xx", 3 },{ "xy", 4 },{ "xz", 5 } };
	vector<int> values = { 9, 6, 5, 7, 3, 2 };
	string variable_name;
	cin >> variable_name;
	string temp = "";

	for (auto i : variables_map)
	{
		cout << " Variable :" << i.first << " At adress:" << i.second << " Has value:" << values[i.second];
	}
	if (variables_map.count(variable_name) > 0)
		cout << " Variable :" << variable_name << " At adress:" << variables_map.at(variable_name) << " Has value:" << values[variables_map.at(variable_name)];
	else
		cout << "Variable " << variable_name << "  doesn't exist" << endl;
	system("pause");
	return 0;
}
