#include "stdafx.h"
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

//your code
void print(const int a )
{
	cout << a;
}
int main()
{
	vector<int> results(20);
	for (vector<int>::iterator i = results.begin(); i != results.end(); ++i)
	{
		if (i != results.begin())
		{
			*i = *(i-1) + 1;
		}
		cout << *i;
	}
	cout << endl;
	vector<int> ::iterator it1 = lower_bound(results.begin(), results.end(), 5);
	vector<int> ::iterator it2 = upper_bound(results.begin(), results.end(), 11);

	//your code
	for_each(it1, it2, print);

	system("pause");
	return 0;
}
